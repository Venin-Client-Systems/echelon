{
  "name": "echelon",
  "version": "0.1.0",
  "description": "Hierarchical multi-agent AI org orchestrator",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Venin-Client-Systems/echelon.git"
  },
  "keywords": ["ai", "orchestrator", "multi-agent", "claude", "cli"],
  "files": ["dist", "ralphy", "README.md", "LICENSE"],
  "bin": {
    "echelon": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs');const f='dist/index.js';let s=fs.readFileSync(f,'utf8');if(s.startsWith('#!'))s=s.slice(s.indexOf('\\n')+1);fs.writeFileSync(f,'#!/usr/bin/env node\\n'+s);fs.chmodSync(f,0o755)\"",
    "dev": "tsx src/index.ts",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "ink": "^5.2.0",
    "nanoid": "^5.1.2",
    "react": "^18.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20"
  }
}
